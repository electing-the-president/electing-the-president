<nav id="mobileMenu">
    <ul>
      {{#link-to 'candidates' tagName="li" href=false}}
        <a {{bind-attr href="view.href"}}>
          <i class="glyphicon glyphicon-user" aria-hidden="true"> </i>
            The Candidates
          </a>
      {{/link-to}}
      {{#link-to 'questions.index' tagName="li" href=false}}
        <a {{bind-attr href="view.href"}}>
          <i class="glyphicon glyphicon-comment" aria-hidden="true"> </i>
            Ask Watson
          </a>
      {{/link-to}}
      {{#link-to 'insights' tagName="li" href=false}}
        <a {{bind-attr href="view.href"}}>
          <i class="glyphicon glyphicon-thumbs-up" aria-hidden="true"> </i>
            Personality Insights
          </a>
      {{/link-to}}
      {{#link-to 'about' tagName="li" href=false}}
        <a {{bind-attr href="view.href"}}>
          <i class="glyphicon glyphicon-info-sign" aria-hidden="true"> </i>
            About
          </a>
      {{/link-to}}
    </ul>
</nav>
<header role="banner" class="navbar navbar-fixed-top navbar-default">
      <div class="container-fluid">
        <div class="navbar-header">
          <h1 id="mobileMenuButton" style="margin-top: 5px;" class="glyphicon glyphicon-th-list pull-left btn visible-xs" aria-hidden="true" {{action 'toggleMobileMenu'}}>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </h1>
          {{#link-to 'index' tag='h1' classNames="logo-text pull-left btn"}}
          <img alt="Electing The President" src="/flagman.png" class="img-responsive pull-left"> <span class="text-danger">E</span><span style="color: white">T</span><span class="text-info">P</span>
        {{/link-to}}
        </div>
        <div class="navbar-default side-collapse hidden-xs">
          <nav role="navigation" class="navbar-collapse">
            <ul class="nav navbar-nav">
              {{#link-to 'candidates' tagName="li" href=false}}
                <a {{bind-attr href="view.href"}}>
                  <span class="glyphicon glyphicon-user" aria-hidden="true"> </span>
                    The Candidates
                  </a>
              {{/link-to}}
              {{#link-to 'questions.index' tagName="li" href=false}}
                <a {{bind-attr href="view.href"}}>
                  <span class="glyphicon glyphicon-comment" aria-hidden="true"> </span>
                    Ask Watson
                  </a>
              {{/link-to}}
              {{#link-to 'insights' tagName="li" href=false}}
                <a {{bind-attr href="view.href"}}>
                  <span class="glyphicon glyphicon-thumbs-up" aria-hidden="true"> </span>
                    Personality Insights
                  </a>
              {{/link-to}}
            </ul>
            <ul class="nav navbar-nav navbar-right">
              {{#link-to 'about' tagName="li" href=false}}
                <a {{bind-attr href="view.href"}}>
                  <span class="glyphicon glyphicon-info-sign" aria-hidden="true"> </span>
                    About
                  </a>
              {{/link-to}}
            </ul>
            <ul class="nav navbar-nav navbar-right hidden">
              {{#if session.isAuthenticated}}
                {{#link-to 'users.index' tagName="li" href=false}}
                  <a {{bind-attr href="view.href"}}>
                    Users
                  </a>
                {{/link-to}}
                {{#link-to 'admin.models.index' tagName="li" href=false}}
                  <a {{bind-attr href="view.href"}}>
                    Models
                  </a>
                {{/link-to}}
                <li><a {{action 'invalidateSession'}}>Logout</a></li>
              {{else}}
                {{#link-to 'login' tagName="li" href=false}}
                  <a {{bind-attr href="view.href"}}>
                    Login
                  </a>
                {{/link-to}}
                {{#link-to 'user.register' tagName="li" href=false}}
                  <a {{bind-attr href="view.href"}}>
                    Register
                  </a>
                {{/link-to}}
              {{/if}}
            </ul>
          </nav>
        </div>
      </div>
    </header>
<div class="container">
{{outlet}}
</div>
<div style="position: fixed; bottom: 37px; padding-bottom: 0; margin-bottom: 0;">
  <div id="watson-chat" class="col-xs-12 col-sm-7 col-md-6 col-lg-4" style="min-width: 320px; max-width: 500px;">
        <div id="chat-panel" class="panel panel-primary " style="margin-bottom: 0; padding-right: 0; ">
          <div class="panel-collapse collapse" id="collapseChat">
            <div class="panel-heading" id="accordion">
              <span class="glyphicon glyphicon-comment"></span> Ask Watson
              <div class="btn-group pull-right">
                <a type="button" class="btn btn-default btn-xs" data-toggle="collapse" data-parent="#accordion" href="#collapseChat">
                  <span class="glyphicon glyphicon-chevron-down"></span>
                </a>
              </div>
            </div>
            <div id="qa-chatbox-div" class="panel-body">
              <ul id="qa-chatbox" class="chat">
              </ul>
              {{#if thinking}}
                <h5 class="text-muted">Watson is typing...</h5>
              {{/if}}
            </div>
        </div>
      </div>
    </div>
</div>
<nav class="navbar navbar-inverse navbar-fixed-bottom" style="height: 39px !important; min-height: 39px; padding: 0; margin: 0; overflow: hidden; z-index: 9999999999999999999999999;">
  <div class="container">
    <form id="ask-form" class=" navbar-left" role="search">
      <div class="form-group" style="padding: 0; margin: 0;">
        <div class=" input-group" style="padding: 0; margin: 0;">
          <span class="input-group-btn">
            <button type="button" class="btn btn-primary" style="padding: 8px 12px 16px 12px; border: 0 !important;" {{action 'toggleChat'}}>
              <span class="glyphicon glyphicon-comment"></span>
            </button>
          </span>
          {{input value=question placeholder="What do you want to know?" type="text" class="form-control bottom-element"}}
          <span class="input-group-btn">
            <button type="submit" class="btn btn-default" style="padding-bottom: 4px; margin: 2px; margin-top: -3px;" {{action 'askWatson'}}>Ask!</button>
          </span>
          <!-- /input-group -->
        </div>
      </div>
    </form>
  </div>
</nav>
